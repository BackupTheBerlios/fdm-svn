    LTEXT           "Source Type",IDC_AS_SOURCE_TYPE,107,9,78,8
    COMBOBOX        IDC_AUTOS_SOURCE_TYPE,107,19,78,78,CBS_DROPDOWNLIST | 
                    WS_VSCROLL | WS_TABSTOP,WS_EX_CLIENTEDGE
    LTEXT           "Size",IDC_AS_FILE_SIZE,9,41,14,8
