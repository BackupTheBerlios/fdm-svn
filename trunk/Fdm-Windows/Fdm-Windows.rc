IDD_AUTO_SEARCH_PROPERTIES DIALOGEX 0, 0, 201, 263
STYLE DS_SETFONT | DS_MODALFRAME | DS_FIXEDSYS | WS_POPUP | WS_CAPTION | 
    WS_SYSMENU
CAPTION "Autosearch Properties"
FONT 8, "MS Shell Dlg", 400, 0, 0x1
BEGIN
    DEFPUSHBUTTON   "OK",IDOK,44,241,50,14
    PUSHBUTTON      "Cancel",IDCANCEL,109,241,50,14
    LTEXT           "Search String",IDC_AS_SEARCH,9,9,88,8
